{
  "name": "jsdiff",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "test:unit": "vue-cli-service test:unit",
    "lint": "vue-cli-service lint && prettier --check .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@sentry/browser": "^5.27.4",
    "@sentry/integrations": "^5.27.4",
    "@sentry/tracing": "^5.27.4",
    "@types/chart.js": "^2.9.27",
    "@types/vue-select": "^2.5.1",
    "autocompleter": "^6.0.5",
    "axios": "^0.21.0",
    "chart.js": "^2.9.4",
    "chartjs-adapter-date-fns": "^1.0.0",
    "core-js": "^3.6.5",
    "date-fns": "^2.16.1",
    "faunadb": "^4.0.0",
    "google-trends-api": "^4.9.0",
    "vue": "^2.6.11",
    "vue-select": "^3.10.8"
  },
  "devDependencies": {
    "@types/jest": "^24.0.19",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vercel/node": "^1.8.4",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-typescript": "^4.5.7",
    "@vue/cli-plugin-unit-jest": "^4.5.7",
    "@vue/cli-service": "~4.5.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "@vue/test-utils": "^1.0.3",
    "babel-eslint": "^10.1.0",
    "eslint": "^6.7.2",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-vue": "^6.2.2",
    "fibers": "^5.0.0",
    "prettier": "^2.1.2",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.3",
    "typescript": "~3.9.3",
    "vue-cli-plugin-tailwind": "^1.5.0",
    "vue-eslint-parser": "^7.1.1",
    "vue-template-compiler": "^2.6.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es2020": true,
      "browser": true,
      "node": true
    },
    "parser": "vue-eslint-parser",
    "plugins": [
      "vue",
      "@typescript-eslint"
    ],
    "extends": [
      "eslint:recommended",
      "@vue/typescript",
      "plugin:@typescript-eslint/recommended",
      "plugin:vue/recommended",
      "prettier",
      "prettier/@typescript-eslint",
      "prettier/vue"
    ],
    "parserOptions": {
      "parser": "@typescript-eslint/parser"
    },
    "rules": {
      "@typescript-eslint/ban-ts-ignore": "off",
      "@typescript-eslint/no-use-before-define": "off",
      "no-sparse-arrays": "off"
    },
    "overrides": [
      {
        "files": [
          "**/__tests__/*.{j,t}s?(x)",
          "**/tests/unit/**/*.spec.{j,t}s?(x)"
        ],
        "env": {
          "jest": true
        }
      }
    ]
  },
  "prettier": {
    "singleQuote": true
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
